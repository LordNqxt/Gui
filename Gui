end)

local arlr = false
local arLoop

local function tar(state)
	if state then
		arlr = true
		arLoop = task.spawn(function()
			while arlr do
				pcall(function()
					local char = plr.Character
					if char then
						local v1 = char:FindFirstChild("RagdollDetect")
						if v1 then v1:Destroy() end

						for _, anr in ipairs(char:GetChildren()) do
							if anr.Name == "VelocityDamage" or anr.Name == "GetPicked" then
								anr:Destroy()
							end
						end
					end
				end)
				task.wait(0.2)
			end
		end)
	else
		arlr = false
	end
end

