	Main.Visible = false
	Tools.Visible = false
	Global.Visible = false
	Misc.Visible = false
	Keybinds.Visible = true
end)

local function changeLineColor()
	while true do
		local r = math.random()
		local g = math.random()
		local b = math.random()

		Line.BackgroundColor3 = Color3.fromRGB(r * 255, g * 255, b * 255)
		task.wait(0.8)
	end
end

task.spawn(changeLineColor)
