    loopflingActive = false
    loopflingv2Active = false
end)

local function av(state)
	if state then
		workspace.FallenPartsDestroyHeight = -math.huge
	else
		workspace.FallenPartsDestroyHeight = -500
	end
end

AntiVoid.MouseButton1Click:Connect(function()
	TButton("AntiVoid", fccolor7)
	av(setting.AntiVoid)
end)


local flingConn = nil

local function FlingAura(state)
	if state then
		flingConn = RunService.Heartbeat:Connect(function()
			local char = plr.Character
			if not char then return end
			local hrp = char:FindFirstChild("HumanoidRootPart")
			if not hrp then return end

			for _, v in pairs(plrs:GetPlayers()) do
				if v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
					local targetHRP = v.Character.HumanoidRootPart
					if (hrp.Position - targetHRP.Position).Magnitude <= 10 then
						pcall(function()
							plr.Character.Picking:FireServer(targetHRP, Vector3.new(math.huge, -math.huge, math.huge))
						end)
					end
				end
			end
		end)
	else
		if flingConn then
			flingConn:Disconnect()
			flingConn = nil
		end
	end
end

FlingAura.MouseButton1Click:Connect(function()
	TButton("FlingAura", fccolor8)
	FlingAura(setting.FlingAura)
end)



local autorespawn = false
local rs = game:GetService("RunService")
local vim = game:GetService("VirtualInputManager")

local function clickrespawn()
    pcall(function()
        local b = plr.PlayerGui:WaitForChild("DeathMenu"):WaitForChild("Frame"):WaitForChild("Button"):WaitForChild("TextButton")
        b.Position = UDim2.new(0,0,0,0)
        b.Size = UDim2.new(1,0,1,0)
        b.Text = ""
        b.BackgroundTransparency = 1
        b.ZIndex = 2^31 - 1
        local s = Instance.new("ScreenGui", game:GetService("CoreGui"))
        b.Parent = s
        vim:SendMouseButtonEvent(b.AbsolutePosition.X + b.AbsoluteSize.X/2, b.AbsolutePosition.Y + b.AbsoluteSize.Y/2, 0, true, game, 0)
        vim:SendMouseButtonEvent(b.AbsolutePosition.X + b.AbsoluteSize.X/2, b.AbsolutePosition.Y + b.AbsoluteSize.Y/2, 0, false, game, 0)
        task.wait()
        s:Destroy()
    end)
end

local function autor()
    while autorespawn do
        if plr.Character and plr.Character:FindFirstChild("Humanoid") and plr.Character.Humanoid.Health <= 0 then
            clickrespawn()
            task.wait(1)
        else
            task.wait(0.5)
        end
    end
end

AutoRespawn.MouseButton1Click:Connect(function()
    autorespawn = not autorespawn
    TButton("AutoRespawn", fccolor9)
    if autorespawn then
        task.spawn(autor)
    end
end)



local hidden2on = false

local function rndpos()
	return CFrame.new(
		math.random(-9e8, 9e8),
		-999,
		math.random(-9e8, 9e8)
	)
end

local function runhid()
	while hidden2on do
		local angle = plr.Character and plr.Character:FindFirstChild("ArmAngleChange")
		if angle then
			angle:FireServer(rndpos())
		end
		task.wait(0.15)
	end
end

Hiddenv2.MouseButton1Click:Connect(function()
	hidden2on = not hidden2on
	TButton("Hiddenv2", fccolor10)
	if hidden2on then
		task.spawn(runhid)
	end
end)



