	tfa(setting.FakeAccs)
end)

local aslr = false
local asLoop

local function tasp(state)
	if state then
		aslr = true
		asLoop = task.spawn(function()
			while aslr do
				for _, v in pairs(plrs:GetPlayers()) do
					if v ~= plr and v.Character and v.Character:IsA("Model") then
						for _, x in pairs(v.Character:GetChildren()) do
							if x:IsA("BasePart") then
								x.Massless = true
							end
						end
					end
				end
				task.wait(0.5)
			end
		end)
	else
		aslr = false
	end
end

