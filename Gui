	TButton("AntiRagdoll", fccolor5)
end)


MainB.MouseButton1Click:Connect(function()
	Main.Visible = false
	Tools.Visible = false
	Global.Visible = false
	Misc.Visible = false
	Keybinds.Visible = false

	Main.Visible = true
end)

ToolsB.MouseButton1Click:Connect(function()
	Main.Visible = false
	Tools.Visible = true
	Global.Visible = false
	Misc.Visible = false
	Keybinds.Visible = false
end)

