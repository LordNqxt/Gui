local function TButton(buttonName, colorFrame)
	setting[buttonName] = not setting[buttonName] 
	if setting[buttonName] then
		colorFrame.BackgroundColor3 = Color3.fromRGB(0, 153, 0) 
	else
		colorFrame.BackgroundColor3 = Color3.fromRGB(153, 0, 0) 
	end
end

local afc
local aflr = false

local function taf(state)
	if state then
		local c = plr.Character or plr.CharacterAdded:Wait()

		local function gw(o) return o:FindFirstChild("ConnectedWeld") end
		local function ic(w) return w and w.Part1 and w.Part1:IsDescendantOf(plr.Character) end
		local function d(o) local w = gw(o) if ic(w) then o:Destroy() end end

		afc = workspace.DescendantAdded:Connect(function(o)
			if o.Name == "Stabler" then
				task.spawn(function()
					if gw(o) or o:WaitForChild("ConnectedWeld", 10) then d(o) end
				end)
			end
		end)

		local function mp(p) return p:IsA("BasePart") and p.Name ~= "HumanoidRootPart" end
		local function mm(p) p.Massless = true end
		local function ma(c) for _,v in ipairs(c:GetDescendants()) do if mp(v) then mm(v) end end end

		local function ga(c) return c:FindFirstChild("ArmAngleChange") end
		local function cf()
			return CFrame.new(0.999999583, 0.650959253, 0.0507530943,
				0.994099438, 0.0974220186, 0.0476996154,
				-0.0791255683, 0.952069402, -0.295470744,
				-0.074198693, 0.289953023, 0.954160273)
		end
		local function sa(c) local e = ga(c) if e then e:FireServer(cf()) end end

		local function loop()
			aflr = true
			while aflr and plr.Character == c do
				ma(c)
				sa(c)
				task.wait(0.2)
			end
		end

		plr.CharacterAdded:Connect(function(nc)
			c = nc
			task.spawn(loop)
		end)

		task.spawn(loop)

	else
		aflr = false
		if afc then
			afc:Disconnect()
			afc = nil
		end
	end
end

Antifling.MouseButton1Click:Connect(function()
	TButton("Antifling", fccolor)
	taf(setting.Antifling)
end)

local avlr = false 
local avLoop

local function tav(state)
	local vec0 = Vector3.new()

	if state then
		avlr = true
		avLoop = task.spawn(function()
			while avlr do
				pcall(function()
					plr.Character:FindFirstChild("RagdollDetect"):Destroy()
				end)
				pcall(function()
					local hrp = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
					if hrp then
						local v = hrp.Velocity
						local rv = hrp.RotVelocity

						if v.X >= 30 or v.X <= -30 or v.Y >= 100 or v.Z >= 30 or v.Z <= -30 then
							hrp.Velocity = vec0
						end
						if rv.Magnitude > 100 then
							hrp.RotVelocity = vec0
						end
					end
				end)
				task.wait(0.1)
			end
		end)
	else
		avlr = false
	end
end

AntiVelocity.MouseButton1Click:Connect(function()
	TButton("AntiVelocity", fccolor1)
	tav(setting.AntiVelocity)
end)

local aalr = false 
local aaLoop

local function taa(state)
	if state then
		aalr = true
		aaLoop = game:GetService("RunService").Heartbeat:Connect(function()
			if not aalr then return end
			for _, v in pairs(game.Players:GetPlayers()) do
				if v ~= plr then
					pcall(function()
						local anvil = v.Character and v.Character:FindFirstChild("Anvil")
						if anvil and anvil:FindFirstChild("send") then
							anvil.send:FireServer(CFrame.new(math.huge, -math.huge, math.huge))
						end
					end)
				end
			end
		end)
	else
		aalr = false
		if aaLoop then
			aaLoop:Disconnect()
			aaLoop = nil
		end
	end
end

AntiAnvil.MouseButton1Click:Connect(function()
	TButton("AntiAnvil", fccolor2)
	taa(setting.AntiAnvil)
end)

local falr = false 
local faLoop

local function tfa(state)
	if state then
		falr = true
		faLoop = task.spawn(function()
			while falr do
				pcall(function()
					for _, v in next, plr.Character:GetChildren() do
						if v:IsA("Accessory") and v:FindFirstChild("Handle") and v.Name ~= "FakeAcc" then
							local clone = v:Clone()
							v:Destroy()
							clone.Name = "FakeAcc"
							clone.Parent = plr.Character
						end
					end
				end)
				task.wait(1)
			end
		end)
	else
		falr = false
	end
end


