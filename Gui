local plrs = game:GetService('Players')
local plr = plrs.LocalPlayer

local setting = {
	Antifling = false,
	AntiVelocity = false,
	AntiAnvil = false,
	FakeAccs = false,
	AntiSpinner = false,
	AntiRagdoll = false,
    HackMovement = false,
	AntiVoid = false,
	FlingAura = false,
	AutoRespawn = false,
	Hiddenv2 = false,
}

	local s, r = pcall(function()
		return game.Players.LocalPlayer:WaitForChild("PlayerGui", 3):FindFirstChild(name)
	end)
	return s and r or nil
end

local fling = safeFindGui("GUI") and safeFindGui("GUI"):FindFirstChild("Tabs", true) and safeFindGui("GUI"):FindFirstChild("fling", true)

local function findplr(text)
	text = text:lower()
	for _, v in pairs(plrs:GetPlayers()) do
		if v.Name:lower():sub(1, #text) == text or v.DisplayName:lower():sub(1, #text) == text then
			return v
		end
	end
end

if fling and fling:IsA("TextBox") then
	fling.FocusLost:Connect(function(enterPressed)
		if not enterPressed then return end
		if not fling.Text or fling.Text == "" then return end

		local tar = findplr(fling.Text)
		if not tar or not tar.Character then return end

		local accs = tar.Character:FindFirstChildOfClass("Accessory")
		if accs then
			local handle = accs:FindFirstChild("Handle")
			if handle then
				plr.Character.Picking:FireServer(handle, Vector3.new(9e37, -9e37, 9e37))
				task.wait(0.1)
				plr.Character.PuttingDown:FireServer()
			elseif accs:IsA("BasePart") then
				plr.Character.Picking:FireServer(accs, Vector3.new(9e37, -9e37, 9e37))
				task.wait(0.1)
				plr.Character.PuttingDown:FireServer()
			end
		else
			local hrp = tar.Character:FindFirstChild("HumanoidRootPart")
			if hrp then
				plr.Character.Picking:FireServer(hrp, Vector3.new(9e37, -9e37, 9e37))
				task.wait(0.1)
				plr.Character.PuttingDown:FireServer()
			end
		end

		fling.Text = ""
	end)
end

flingv2.FocusLost:Connect(function(enterPressed)
	if not enterPressed then return end

	local target = findplr(flingv2.Text)
	if not target or not target.Character then return end

	local angle = target.Character:FindFirstChild("ArmAngleChange")
	if angle then
		angle:FireServer(CFrame.new(math.huge, -999, -math.huge))
	end

	flingv2.Text = "" 
end)

local flingtargets = {}
local flingv2targets = {}

local function getplr(name)
	for _, v in ipairs(plrs:GetPlayers()) do
		if v.Name:lower():find(name:lower()) or v.DisplayName:lower():find(name:lower()) then
			return v
		end
	end
end

local function loopflingh()
	while task.wait(0.3) do
		for name, _ in pairs(flingtargets) do
			local tar = getplr(name)
			if tar and tar.Character then
				local accs = tar.Character:FindFirstChildOfClass("Accessory")
				if accs then
					if accs:FindFirstChild("Handle") then
						plr.Character.Picking:FireServer(accs.Handle, Vector3.new(9e37, -9e37, 9e37))
					elseif accs:IsA("BasePart") then
						plr.Character.Picking:FireServer(accs, Vector3.new(9e37, -9e37, 9e37))
					end
				else
					local hrp = tar.Character:FindFirstChild("HumanoidRootPart")
					if hrp then
						plr.Character.Picking:FireServer(hrp, Vector3.new(9e37, -9e37, 9e37))
					end
				end
				task.wait(0.05)
				plr.Character.PuttingDown:FireServer()
			end
		end
	end
end

local function loopflingv2h()
	while task.wait(0.3) do
		for name, _ in pairs(flingv2targets) do
			local tar = getplr(name)
			if tar and tar.Character and tar.Character:FindFirstChild("ArmAngleChange") then
				tar.Character.ArmAngleChange:FireServer(CFrame.new(math.huge, -999, -math.huge))
			end
		end
	end
end

task.spawn(loopflingh)
task.spawn(loopflingv2h)

loopfling.FocusLost:Connect(function()
	local text = loopfling.Text
	if text ~= "" then
		local tar = getplr(text)
		if tar then
			flingtargets[tar.Name] = true
			loopfling.Text = ""
		end
	end
end)

loopflingv2.FocusLost:Connect(function()
	local text = loopflingv2.Text
	if text ~= "" then
		local tar = getplr(text)
		if tar then
			flingv2targets[tar.Name] = true
			loopflingv2.Text = ""
		end
	end
end)

